{
  "title": "webpack-code-splitting",
  "name": "webpack-code-splitting",
  "version": "0.0.0",
  "private": true,
  "repository": "non-public",
  "scripts": {
    "postinstall": "find ./js/* -name package.json -maxdepth 2 -execdir yarn \\;",
    "clean": "find . -name package.json -execdir rm -fr node_modules \\;",
    "build": "yarn; rm -fr build && webpack --progress --bail --config webpack.config.babel.js",
    "dev": "yarn; rm -fr build && webpack-dev-server --open --hot --output-public-path='/build/' --progress --history-api-fallback --compress --config webpack.dev.config.babel.js"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "webpack": "2.5.1"
  }
}
